<Page loaded="loaded">
	<ScrollView>
		<StackLayout orientation ="vertical">
			
			<GridLayout columns="*, *, *, *" rows="auto, auto, auto, auto, auto, auto, auto, auto, auto, auto, auto, auto, auto, auto, auto" verticalAlignment="left" horizontalAlignment="center">
				<Border cssClass="pageHead" row="0" colSpan="4">
					<Label id="navHeader" text="&#60; Choose A Challenge &#62;" horizontalAlignment="center" cssId="banner"/>
				</Border>
				<Border cssClass="listElts" row="1" col="0" rowSpan="2" colSpan="4"></Border>
	    			<Label tap="recursion" row="1" cssClass="boxHeader" colSpan="3" text="Recursion"/>
	    			<Button row="1" colSpan="1" rowSpan="2" col="4" text="&#62;" cssClass="collapseButton" verticalAlignment="center"/>
	    			<Label tap="recursion" cssClass="description" text="description" row="2" colSpan="2"/>

				<Border cssClass="spacer" row="3" col="0" rowSpan="1" colSpan="4"></Border>
					<Label text=" " cssClass="spacer" row="3" col="0" colSpan="4"/>

				<Border cssClass="listElts" row="4" col="0" rowSpan="2" colSpan="4"></Border>
	    			<Label tap="dataStructures" cssClass="boxHeader" row="4" colSpan="3" text="Data Structures"/>
	    			<Button colSpan="1" rowSpan="2" row="4" col="4" text="&#62;" cssClass="collapseButton" verticalAlignment="center"/>
	    			<Label tap="dataStructures" cssClass="description" text="description" row="5" colSpan="2"/>
				
				<Border cssClass="listElts" row="6" col="0" colSpan="4"></Border>
					<Label cssClass="spacer" text=" " row="6" colSpan="4"/>

				<Border cssClass="listElts" row="7" col="0" rowSpan="2" colSpan="4"></Border>
	    			<Label tap="algorithms" cssClass="boxHeader" row="7" colSpan="3" text="Algorithms"/>
	    			<Button colSpan="1" rowSpan="2" row="7" col="4" text="&#62;" cssClass="collapseButton" verticalAlignment="center"/>
	    			<Label tap="algorithms" cssClass="description" text="description" row="8" colSpan="2"/>

				<Border cssClass="listElts" row="9" col="0" colSpan="4"></Border>
					<Label cssClass="spacer" text=" " row="9" colSpan="4"/>

				<Border cssClass="listElts" row="10" col="0" rowSpan="2" colSpan="4"></Border>
	    			<Label tap="oo" cssClass="boxHeader" row="10" colSpan="3" text="Object-Oriented Design"/>
	    			<Button colSpan="1" rowSpan="2" row="10" col="4" text="&#62;" cssClass="collapseButton" verticalAlignment="center"/>
	    			<Label tap="oo" cssClass="description" text="description" row="11" colSpan="2"/>

				<Border cssClass="listElts" row="12" col="0" colSpan="4"></Border>
					<Label cssClass="spacer" text=" " row="12" colSpan="4"/>

				<Border cssClass="listElts" row="13" col="0" rowSpan="2" colSpan="4"></Border>
	    			<Label tap="bigO" cssClass="boxHeader" row="13" colSpan="3" text="Big O Notation"/>
	    			<Button colSpan="1" rowSpan="2" row="13" col="4" text="&#62;" cssClass="collapseButton" verticalAlignment="center"/>
	    			<Label tap="bigO" cssClass="description" text="description" row="14" colSpan="2"/>


	    	</GridLayout>

			
			<!-- <Button text="Sign in" cssClass="link" tap="signIn" /> -->
		</StackLayout>
	</ScrollView>

</Page>