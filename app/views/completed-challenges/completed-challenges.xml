<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" actionBarHidden="false">
	<Page.actionBar>
		<ActionBar title="{{ title }}"/>
	</Page.actionBar>

  <ScrollView>
    <Repeater items="{{ completed_challenge_list }}">
      <Repeater.itemsLayout>
        <StackLayout orientation="vertical" />
      </Repeater.itemsLayout>
      <Repeater.itemTemplate>
        <WrapLayout>
          <Label text="{{ title }}" />
          <Label text="{{ user_score, ' Score: ' + user_score  }}"/>
          <Label text="{{ max_points, ' Max: ' + max_points  }}" />
        </WrapLayout>
      </Repeater.itemTemplate>
    </Repeater>
  </ScrollView>
  
</Page>
