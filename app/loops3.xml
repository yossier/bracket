<Page loaded="loaded">
	<StackLayout orientation="vertical">
		<!-- Line 1 - Function Declaration -->
		<WrapLayout>
			<Label cssClass= "type" text="int " />
			<Label cssClass= "functionTitle" text="lastIncreasing"/>
			<Label cssClass= "plain" text="("/>
			<Label cssClass="type" text="int"/>
			<Label cssClass="plain" text=" array[], "/>
			<Label cssClass="type" text="int "/>
			<Label cssClass="plain" text="size){"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass= "type indent1" text="int " />
			<Label cssClass="plain" text="lastIndexNum = "/>
			<TextField id="lastIndexNum" cssClass="oneChar" hint="??" text="{{ lastIndexNum }}"/>
			<Label cssClass="plain" text=";"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="statement indent1" text="for"/>
			<Label cssClass="plain" text=" ("/>
			<Label cssClass="type" text="int "/>
			<Label cssClass="plain" text="i = "/>
			<Label cssClass="number" text="0"/>
			<Label cssClass="plain" text="; i < "/>
			<TextField id="condition" cssClass="big" hint="?????" text="{{ condition }}"/>
			<Label cssClass="plain" text="; ++i){"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="statement indent2" text="if "/>
			<Label cssClass="plain" text="(array[i - "/>
			<Label cssClass="number" text="1"/>
			<Label cssClass="plain" text="] < "/>
			<TextField id="arri" cssClass="big" hint="???????" text = "{{ arri }}"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="plain indent2" text="&amp;&amp; array[i] < "/>
			<TextField id="arrip1" cssClass="big" hint="???????" text = "{{ arrip1 }}"/>
			<Label cssClass="plain" text="){"/>
		</WrapLayout>
		<WrapLayout>
			<TextField id="equalTo" cssClass="big indent3" hint="???????" text = "{{ equalTo }}"/>
			<Label cssClass="plain" text=" = i;"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="plain indent2" text="}"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="plain indent1" text="}"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="statement indent1" text= "return "/>
			<TextField id="returnVal" cssClass="big" hint="?????" text="{{ returnVal }}"/>
			<Label class="plain" text=";"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="plain" text="}"/>
		</WrapLayout>
		<Button text="Print!" tap="print"/>
	</StackLayout>
</Page>