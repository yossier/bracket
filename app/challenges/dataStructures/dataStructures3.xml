<Page loaded="loaded">
  <ScrollView>
	<StackLayout orientation="vertical">
		<WrapLayout>
            <Label cssClass="description1 indent0" text="/* GOAL: Declare a 5x5 2-D array of ints,"/>
        </WrapLayout>
        <WrapLayout>
            <Label cssClass="description2 indent0" text="* Next, initialize all values to -1."/>
        </WrapLayout>
        <WrapLayout>
            <Label cssClass="description2 indent0" text="* Then, change the center elt to a 9."/>
        </WrapLayout>
        <WrapLayout>
            <Label cssClass="description3 indent0" text="*/"/>
        </WrapLayout>
		<!-- Line 1 - Function Declaration -->
		<WrapLayout>
			<Label cssClass="type" text="int " />
			<Label cssClass="functionTitle" text="main"/>
			<Label cssClass="plain" text="(){"/>
		</WrapLayout>

		<WrapLayout></WrapLayout>

		<WrapLayout>
			<TextField id="intArray" cssClass="fiveChar indent1" hint="??" text="{{ intArray }}"/>
			<Label cssClass="plain" text=" myarray["/>
			<Label cssClass="number" text="5"/>
			<Label cssClass="plain" text="]"/>
			<Label cssClass="plain" text="["/>
			<TextField id="five" cssClass="oneChar" hint="??" text="{{ five }}"/>
			<Label cssClass="plain" text="]"/>
			<Label cssClass="plain" text=";"/>
		</WrapLayout>

		<WrapLayout></WrapLayout>

		<WrapLayout>
			<Label cssClass="statement indent1" text="for"/>
			<Label cssClass="plain" text=" ("/>
			<Label cssClass="type" text="int "/>
			<Label cssClass="plain" text="i = "/>
			<Label cssClass="number" text="0"/>
			<Label cssClass="plain" text="; i < "/>
			<TextField id="condition5" cssClass="oneChar" hint="??" text="{{ condition5 }}"/>
			<Label cssClass="plain" text="; ++i){"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="statement indent2" text="for"/>
			<Label cssClass="plain" text=" ("/>
			<Label cssClass="type" text="int "/>
			<Label cssClass="plain" text="j = "/>
			<TextField id="setJ5" cssClass="oneChar" hint="??" text="{{ setJ5 }}"/>
			<Label cssClass="plain" text="; j < "/>
			<Label cssClass="number" text="5"/>
			<Label cssClass="plain" text="; ++j){"/>
		</WrapLayout>
		<WrapLayout></WrapLayout>
		<WrapLayout>
			<Label cssClass="plain indent3" text=" myarray["/>
			<TextField id="indexI" cssClass="twoChar" hint="??" text="{{ indexI }}"/>
			<Label cssClass="plain" text="]"/>
			<Label cssClass="plain" text="["/>
			<TextField id="indexJ" cssClass="twoChar" hint="??" text="{{ indexJ }}"/>
			<Label cssClass="plain" text="] = "/>
			<TextField id="neg1" cssClass="twoChar" hint="??" text="{{ neg1 }}"/>
			<Label cssClass="plain" text=";"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="plain indent2" text="}"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="plain indent1" text="}"/>
		</WrapLayout>

		<WrapLayout></WrapLayout>

		<WrapLayout>
			<Label cssClass="plain indent1" text=" myarray["/>
			<TextField id="indexArr1" cssClass="twoChar" hint="??" text="{{ indexArr1 }}"/>
			<Label cssClass="plain" text="]"/>
			<Label cssClass="plain" text="["/>
			<TextField id="indexArr2" cssClass="twoChar" hint="??" text="{{ indexArr2 }}"/>
			<Label cssClass="plain" text="] = "/>
			<Label cssClass="number" text="9"/>
			<Label cssClass="plain" text=";"/>
		</WrapLayout>
		<WrapLayout>
			<Label cssClass="plain" text="}"/>
		</WrapLayout>
		<Button text="Submit" cssClass="submitButton" tap="print"/>
	</StackLayout>
</ScrollView>
</Page>
